@model Projet2.Models.PanierSaisonniers.PanierSaisonnier
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ModifierPanier</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <fieldset>
            <legend>Modifier un PAnier : </legend>
            @Html.LabelFor(m => Model.ProduitsProposes)
            @Html.TextBoxFor(m => Model.ProduitsProposes)
            @Html.ValidationMessageFor(m => Model.ProduitsProposes)
            <br />
            @Html.LabelFor(m => Model.Telephone)
            @Html.TextBoxFor(m => Model.Telephone)
            @Html.ValidationMessageFor(m => Model.Telephone)
            <br />
            <input type="submit" value="Modifier" />
        </fieldset>
    }

    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/jquery.validate-vsdoc.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

</body>
</html>